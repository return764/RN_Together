    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
