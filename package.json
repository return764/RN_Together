    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-reanimated": "^2.9.1",
    "react-native-reanimated-carousel": "^3.0.3",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.16.0",
    "react-native-svg": "^12.4.3"
  },
  "devDependencies": {
